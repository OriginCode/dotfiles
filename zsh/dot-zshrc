# History Stamp
HIST_STAMPS="yyyy/mm/dd"

# Coloured man pages using less as pager
man() {
    env \
	      LESS_TERMCAP_mb=$(printf "\e[1;31m") \
	      LESS_TERMCAP_md=$(printf "\e[1;31m") \
	      LESS_TERMCAP_me=$(printf "\e[0m") \
	      LESS_TERMCAP_se=$(printf "\e[0m") \
	      LESS_TERMCAP_so=$(printf "\e[1;44;33m") \
	      LESS_TERMCAP_ue=$(printf "\e[0m") \
	      LESS_TERMCAP_us=$(printf "\e[1;36m") \
	      man "$@"
}

# Show info when start up
# neofetch

# fars.ee upload func
function farsee() {
    curl -F sunset=86400 -sF "c=@${1:--}" -w "%{redirect_url}" 'http://fars.ee/?r=1' -o /dev/stderr | xsel -l /dev/null -b
}

function farsee_etern() {
    curl -sF "c=@${1:--}" -w "%{redirect_url}" 'http://fars.ee/?r=1' -o /dev/stderr | xsel -l /dev/null -b
}

